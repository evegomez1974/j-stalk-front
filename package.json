{
  "name": "j-stalk_front",
  "version": "2.4.5",
  "main": "nuxt.config.js",
  "private": false,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "release": "semantic-release",
    "generate": "nuxt generate",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .test.js,.js,.jsx,.cjs --fix --ignore-path .gitignore"
  },
  "release": {
    "branches": [
      "develop"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "files": [
    ".nuxt/",
    "README.md"
  ],
  "dependencies": {
    "@nuxtjs/composition-api": "^0.33.1",
    "@pdftron/webviewer": "^7.1.0",
    "@pinia/nuxt": "^0.2.1",
    "@semantic-release/changelog": "^6.0.3",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.0",
    "core-js": "^3.25.3",
    "nuxt": "^2.15.8",
    "pdfjs-dist": "^3.4.120",
    "pinia": "^2.0.36",
    "pinia-orm": "^1.6.7",
    "vite": "^5.0.10",
    "vue": "^2.7.10",
    "vue-demi": "^0.14.1",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10",
    "vue2-editor": "^2.10.3"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.2",
    "@types/pdfjs-dist": "^2.10.378",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jest": "^29.7.0",
    "jest-unit": "^0.0.2",
    "sass": "^1.57.1",
    "sass-loader": "^10.4.1",
    "semantic-release": "^23.0.2",
    "vitest": "^0.34.6"
  }
}
